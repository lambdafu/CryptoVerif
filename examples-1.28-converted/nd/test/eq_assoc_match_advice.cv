type D.

(* set interactiveMode = true. *)

fun concat(D,D):D.
equation assoc(concat).

fun f(D):bitstring.
fun h(D):bitstring.
fun g(D,D,D): bitstring.

const zero, one, two, three: D. 

channel c.

proba Peq.
param n.

equiv
  ! n O(x: D, y: D) := return(h(concat(x,concat(y,x)))) [all]
<=(Peq)=>
  ! n O(x: D, y: D) := return(g(x,y,x)).

process in(c, ());
out(c, h(concat(concat(zero, one), concat(two, zero))));
in(c, ());
let z: D = concat(zero, one) in
out(c, h(concat(z, concat(two, zero))));
in(c, ());
let t: D = concat(two, zero) in
out(c, h(concat(z, t)));
in(c, ());
let x: D = concat(z, t) in
out(c, h(x))




type D.

(* set interactiveMode = true. *)

fun mult(D,D):D.
fun inv(D):D.
const neut:D.
equation builtin group(mult,inv,neut).

fun h(D):bitstring.
fun g(D,D,D): bitstring.

const zero, one, two, three: D. 

channel c.
proba Peq.
param n.

equiv
  ! n O(x: D, y: D) := return(h(mult(x,mult(y,x)))) [all]
<=(Peq)=>
  ! n O(x: D, y: D) := return(g(x,y,x)).


process in(c, ());
out(c, h(inv(mult(mult(zero, one), mult(two, zero)))));
in(c, ());
out(c, h(mult(mult(zero, one), mult(two, mult(zero, one)))));
in(c, ());
let z: D = mult(zero, one) in
out(c, h(mult(z, mult(two, zero))));
in(c, ());
let t: D = mult(two, zero) in
out(c, h(mult(z, t)));
in(c, ());
let x: D = inv(mult(z, t)) in
out(c, h(x))





new test: S;
out(dummy, test);
! i_1 <= q
in(dummy[i_1], x: D);
new b: S;
out(dummy[i_1], f(b, x));
in(dummy[i_1], y: D);
new a: S;
out(dummy[i_1], (g(a, b, y), h(a, y)))






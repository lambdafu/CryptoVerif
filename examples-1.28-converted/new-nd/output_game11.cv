             if check(eb1, Kmkey[jB_369], mb1) then
             let injbot(tagf(=tag2, concat1(Nb_389: nonce, =M_387, =ha_386, =hb_385))) = dec(eb1, Rkey[jB_369]) in
             new k_536: key;
             new s3_537: seed;

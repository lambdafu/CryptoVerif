               let e1: maxmac = enc(tagf(tag3, concat2(Na_388, k_539)), Rkey[jA_370], s3_540) in
               let e2_391: maxmac = enc(tagf(tag4, concat2(Nb_380[u_860], k_539)), Rkey[jB_369], s4_541) in

type Z [large,bounded].
type G [large,bounded].

(* Let's use a good group *)
expand DH_good_group(G, Z, g, exp, exp', mult).

channel c.

event bad.
query event(bad) ==> false.

process
	in(c, (X: G, X': G, y:Z)); if exp(X,y) = exp(X',y) && X <> X' then event bad


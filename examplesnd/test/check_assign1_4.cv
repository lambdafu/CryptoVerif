   (It could not in version 2.01pl1) *)


type t1.
type t2 [large].
type nonce [fixed,large].

fun concat(t1,t1):t2 [data].
fun concat2(t1,t2):nonce [data].

channel c,d.

event e1.

query event(e1).

process
	in(c, (a1: t1, a2:t1));
	new b: nonce;
	let concat2(x, concat(=a1,=a2)) = b in
	event e1


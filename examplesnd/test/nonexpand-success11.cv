set autoExpand = false.

param N.

type nonce [fixed,large].

event e1(nonce).
event e2(nonce).

query secret x.

channel c.

proof {
success
}

process
   !N in(c,(b: bool, z: nonce, t:nonce)); let a = new y: nonce; let x: nonce = y in if b then event e1(z); z else t in event e2(if defined(x) && b then x else t); out(c, z)


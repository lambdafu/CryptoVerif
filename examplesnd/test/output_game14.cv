         let x_2127: maxmac = ea1 in
         out(c2[i_363], (M_373, A, hb_372, ea1, mac2_323(x_2127, mkgen2_325(rmKas))));
         in(c3[i_363], (=M_373, ea2_376: maxmac, ma2_375: macs));
         let m_2126: maxmac = ea2_376 in
         let ma_2125: macs = ma2_375 in

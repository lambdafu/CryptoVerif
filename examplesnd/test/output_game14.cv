       ) | (
         ! i_368 <= N
         in(c9[i_368], (M_387: nonce, ha_386: host, hb_385: host, ea1_384: maxmac, ma1: macs, eb1: maxmac, mb1: macs));
         find jA_370 = jA <= N2 suchthat defined(Kmkey[jA], Rmkey_489[jA], Rkey[jA], Khost[jA]) && (ha_386 = Khost[jA]) then
